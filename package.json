  "scripts": {
    "build": "rm -rf lib && tsc -p ./tsconfig.build.json && yarn build:cjs && cp README.md lib",
    "build:cjs": "node build-cjs.js",
    "test": "cross-env NODE_ENV=production yarn node --experimental-vm-modules $(yarn bin jest)",
    "dev": "run-p -r \"dev:build watch\" \"dev:serve\"",
    "dev:build": "rm -r dist || true && tsx examples/build.ts",
    "dev:serve": "serve .",
    "dev:prod": "yarn dev:build && yarn dev:serve",
    "npm-version": "yarn build && node version.js"
  },
  "devDependencies": {
    "@lit-labs/react": "^1.1.1",
