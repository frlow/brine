    "@types/ws": "^8.5.3",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.7",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-vue3": "^0.3.0",
    "esbuild-svelte": "^0.7.3",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
